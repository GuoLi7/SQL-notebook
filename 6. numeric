--  numeric functions 数值函数 Google查表
-- 四舍五入function
SELECT ROUND(5.73235);
SELECT ROUND(5.73235, 1);
SELECT ROUND(5.73235, 2);
-- 直接保留function
SELECT TRUNCATE(5.73235, 2)
SELECT TRUNCATE(5.78235, 1)
-- CEILING&FLOOR
SELECT CEILING(5.7)
SELECT FLOOR (5.7)
-- 计算绝对值ABS
SELECT ABS(-5.2)
-- 生成0-1之间的随机数
SELECT RAND()
-- 生成1-10之间的随机整数
SELECT CEILING (10*RAND())
